algoritmo "ex041"
// 41) Escreva um programa que mostre na tela a seguinte contagem:
//     100 95 90 85 80 ... 0 Acabou!
var
numero: inteiro

inicio

      numero <- 100
      
      Enquanto numero >= 0 faca
               Escreva (numero)
               numero <- numero - 5
      fimEnquanto
      
      Escreva (" Acabou!")


fimalgoritmo
