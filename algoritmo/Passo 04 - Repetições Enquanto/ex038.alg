algoritmo "ex038"
// 38) Escreva um programa que mostre na tela a seguinte contagem:
//     6 7 8 9 10 11 Acabou!
var
numero: inteiro

inicio

      numero <- 6
      
      Enquanto numero <= 11 faca
               Escreva (numero)
               numero <- numero + 1
      fimEnquanto
      
      Escreva (" Acabou!")


fimalgoritmo
