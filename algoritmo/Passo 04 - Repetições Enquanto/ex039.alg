algoritmo "ex039"
// 39) Escreva um programa que mostre na tela a seguinte contagem:
//     10 9 8 7 6 5 4 3 Acabou!
var
numero: inteiro

inicio

      numero <- 10
      
      Enquanto numero >= 3 faca
               Escreva (numero)
               numero <- numero - 1
      fimEnquanto
      
      Escreva (" Acabou!")


fimalgoritmo
