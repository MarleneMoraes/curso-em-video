algoritmo "ex040"
// 40) Escreva um programa que mostre na tela a seguinte contagem:
//     0 3 6 9 12 15 18 Acabou!
var
numero: inteiro

inicio

      numero <- 0
      
      Enquanto numero <= 18 faca
               Escreva (numero)
               numero <- numero + 3
      fimEnquanto
      
      Escreva (" Acabou!")


fimalgoritmo
